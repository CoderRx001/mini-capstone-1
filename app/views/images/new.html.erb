<h1>Add Photo to <%= @product.name %></h1>

<%= form_tag "/products/#{@product.id}/images", method: :post do %>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div>
    <%= label_tag :url %>
    <%= text_field_tag :url %>
  </div>
  <%= submit_tag "Add Photo" %>
<% end %>